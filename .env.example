# WARP Pool Configuration Template
# Copy this file to .env and modify as needed

# Application Version (managed by pyproject.toml, for reference only)
APP_VERSION=1.3

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false

# WARP Configuration
WARP_BACKEND=official
WARP_PROTOCOL=masque

# Logging
LOG_LEVEL=INFO

# Frontend
FRONTEND_URL=http://localhost:5173

# API Settings
API_TIMEOUT=30
MAX_CONNECTIONS=100
